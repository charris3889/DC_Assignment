<Window x:Class="ChatUserOne.LoginPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatUserOne"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="UsernameBox" VerticalAlignment="Center" Text="Username" Margin="290,0,290,0" Height="26"/>
        <Button Name="LoginButton" Content="Login" VerticalAlignment="Top" Click="LoginButton_Click" Height="33" Margin="0,401,0,0"/>

    </Grid>
</Window>