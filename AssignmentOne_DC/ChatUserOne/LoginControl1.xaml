<UserControl x:Class="ChatUserOne.LoginControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChatUserOne"
             mc:Ignorable="d" 
             Background="Wheat"
             VerticalAlignment="Stretch"
             HorizontalAlignment="Stretch">
    <Grid>
        <Button Name="LoginButton" Content="Button" HorizontalAlignment="Center" Margin="0,274,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.266,0.664" Click="LoginButton_Click"/>
        <Label Content="Login page" HorizontalAlignment="Left" Margin="380,103,0,0" VerticalAlignment="Top"/>
        <TextBox Name="UsernameBox" HorizontalAlignment="Left" Margin="355,192,0,0" TextWrapping="Wrap" Text="Enter username here" VerticalAlignment="Top" Width="120"/>
    </Grid>
</UserControl>
