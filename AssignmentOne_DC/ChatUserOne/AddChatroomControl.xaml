<UserControl x:Class="ChatUserOne.AddChatroomControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChatUserOne"
             mc:Ignorable="d" 
             Background="Azure"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button Name="CreateButton" Content="Create" HorizontalAlignment="Center" Margin="0,285,0,0" VerticalAlignment="Top" Click="createButton_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="380,150,0,0" TextWrapping="Wrap" Text="Create chatroom" VerticalAlignment="Top"/>
        <TextBox Name="ChatnameBox" HorizontalAlignment="Left" Margin="345,225,0,0" TextWrapping="Wrap" Text="Enter chatroom here" VerticalAlignment="Top" Width="120"/>
    </Grid>
</UserControl>
